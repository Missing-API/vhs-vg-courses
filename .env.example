# Logging configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_DESTINATION=console
LOG_REQUEST_DETAILS=true
# LOG_FILE_PATH=./logs/app.log

# Node.js environment
NODE_ENV=development

# VHS website client configuration
VHS_REQUEST_TIMEOUT=5000
VHS_CACHE_WARMING_ENABLED=true

# VHS session management
VHS_HEALTH_TIMEOUT_MS=8000
VHS_SESSION_TIMEOUT=900000
VHS_COOKIE_DEBUG=false
VHS_SESSION_FALLBACK=false

# HTTP caching configuration
HTTP_CACHE_DURATION_SECONDS=86400